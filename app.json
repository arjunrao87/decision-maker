{
  "expo": {
    "name": "The One",
    "description": "Makes picking places to go to so easy!",
    "slug": "the-one",
    "privacy": "public",
    "sdkVersion": "19.0.0",
    "version": "1.1.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app.png",
    "loading": {
      "icon": "./assets/icons/loading.png",
      "hideExponentText": true
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.tnc.theone",
    },
    "android": {
      "package": "org.tnc.theone",
      "permissions" : ["ACCESS_FINE_LOCATION"]
    }
  }
}
